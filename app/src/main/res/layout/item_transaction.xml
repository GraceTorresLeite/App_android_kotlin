<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/transactionCategoriaEnumTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionIdTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionDataTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionValorTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionDescricaoTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionContaTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionTipoTransicaoEnumTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

        <TextView
            android:id="@+id/transactionTipoRendaEnumTV"
            android:layout_width="83dp"
            android:layout_height="61dp"
            android:padding="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true">
        </TextView>

    </RelativeLayout>

</com.google.android.material.card.MaterialCardView>